{
  "name": "lightning-pool",
  "description": "Fastest generic Pool written with TypeScript",
  "version": "3.1.4",
  "author": "Panates Ltd.",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>"
  ],
  "license": "MIT",
  "repository": "panates/lightning-pool",
  "keywords": [
    "javascript",
    "typescript",
    "pool",
    "generic"
  ],
  "dependencies": {
    "doublylinked": "^2.5.1",
    "putil-promisify": "^1.8.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "dotenv": "^16.0.1",
    "eslint": "^8.20.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^10.0.0",
    "npm-run-path": "^5.1.0",
    "nyc": "^15.1.0",
    "ts-cleanup": "^0.2.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 10.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "test"
  },
  "files": [
    "LICENSE",
    "README",
    "dist/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output"
  },
  "scripts": {
    "test": "TS_NODE_PROJECT='./test/tsconfig.json' mocha -r ts-node/register --reporter spec test/**/*.spec.ts",
    "cover": "nyc --reporter=cobertura --reporter html --reporter text npm run test",
    "clean": "npm run clean:src && npm run clean:dist",
    "clean:dist": "ts-cleanup -d dist --remove-dirs --all",
    "clean:src": "ts-cleanup -s src --all | ts-cleanup -s test",
    "build": "tsc -b tsconfig-build.json",
    "compile": "tsc -b tsconfig.json",
    "lint": "eslint src/** --no-error-on-unmatched-pattern",
    "travis-cover": "nyc --reporter lcovonly npm run test"
  }
}
